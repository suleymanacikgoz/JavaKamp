package donduncuGunBirinciOdev;

import java.rmi.RemoteException;

import tr.gov.nvi.tckimlik.WS.KPSPublicSoapProxy;

public class Main {

	public static void main(String[] args) throws RemoteException {
		BaseCustomerManager customerManager = new NeroCustomerManager();
		Customer customer1 = new Customer();
		customer1.DateOfBirth=123;
		customer1.FirstName="Süleyman";
		customer1.Id=1;
		customer1.LastName="Açýkgöz";
		customer1.NationalityId="sadasd";
		customerManager.Save(customer1);
		
		
		KPSPublicSoapProxy kpsPublic = new KPSPublicSoapProxy();      
		boolean result = kpsPublic.TCKimlikNoDogrula(1234, "Süleyman", "Açýkgöz", 2000);
		
		System.out.println("Doðrulama : " + (result ? "baþarýlý.":"baþarýsýz.")); 
				
		
	}

}
